function greet(input){
    
    let a=Number(input[0]);
    
    let bonus;
    if(a%2===0){
      bonus=1
    }else if(a%10===5){
      bonus=2
    }else{
      bonus=0
    }

    if(a<=100){
      bonus=bonus+5;
     ;
    }else if(a<=1000){
      bonus=bonus+a*0.2
      
    }else{
      bonus=bonus+a*0.1
    }

    console.log(bonus);
    console.log(a+bonus)
    
}
