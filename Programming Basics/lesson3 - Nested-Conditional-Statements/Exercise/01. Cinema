function greet(input){
   
  let type=input[0];
  let rows=Number(input[1]);
  let lines=Number(input[2]);
  let total=0;

  switch (type) {
    case "Premiere":
      total=rows*lines*12
      break;
    case "Normal":
      total=rows*lines*7.5
      break;
    case "Discount":
      total=rows*lines*5
      break;
  
    default:
      break;
  }
 console.log(`${total.toFixed(2)} leva`);
  
}
