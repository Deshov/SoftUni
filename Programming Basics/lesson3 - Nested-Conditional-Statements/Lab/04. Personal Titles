function greet(input){
    
  let age=Number(input[0]);
  let pol=input[1];
  if (age<16) {
    switch (pol) {
      case "m":
        console.log("Master");
        break;
      case "f":
        console.log("Miss");
        break;
    }
  }else{
    switch (pol) {
      case "m":
        console.log("Mr.");
        break;
      case "f":
        console.log("Ms.");
        break;
    }
  }
  
}
