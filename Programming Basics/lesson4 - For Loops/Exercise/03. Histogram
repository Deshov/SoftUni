function greet(input) {
    let numbers = Number(input[0])
    let currentNumb=0;
     let c1=0;
     let c2=0;
     let c3=0;
     let c4=0;
     let c5=0;
     let p1=0;
     let p2=0;
     let p3=0;
     let p4=0;
     let p5=0;
    
  for (let i = 1; i<input.length; i++) {
         currentNumb=Number(input[i])
        
         if (currentNumb<200) {
             c1+=1;
         }else if(currentNumb<=399){
            c2+=1;
         }else if(currentNumb<=599){
            c3+=1;
         }else if(currentNumb<=799){
            c4+=1;
         }else{
            c5+=1;
         }
        
  }
  p1=c1/numbers*100
  p2=c2/numbers*100
  p3=c3/numbers*100
  p4=c4/numbers*100
  p5=c5/numbers*100

  console.log(`${p1.toFixed(2)}%`);
  console.log(`${p2.toFixed(2)}%`);
  console.log(`${p3.toFixed(2)}%`);
  console.log(`${p4.toFixed(2)}%`);
  console.log(`${p5.toFixed(2)}%`);
 
}
