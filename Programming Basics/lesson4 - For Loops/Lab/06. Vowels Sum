function greet(input) {
    let n = input[0]
    let total=0;
  for (let index = 0; index<n.length; index++) {
      
    
    switch (n[index]) {
        case "a":
             total+=1;
            break;
        case "e":
             total+=2;
            break;
        case "i":
             total+=3;
            break;
        case "o":
             total+=4;
            break;
        case "u":
             total+=5;
            break;
    
    } 
  }
  console.log(total);
}
